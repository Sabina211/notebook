@model Notebook.Views.MainTable.ContactModel
@{  
    }
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Notebook</title>
</head>
<body>
    <table cellspacing="0" border="1" cellpadding="3">
        <caption>Контакт @Model.CurrentContact.Name @Model.CurrentContact.Patronymic</caption>
        <tr>
            <th>Id</th>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <th>Имя</th>
            <td>@Model.CurrentContact.Name</td>
        </tr>
        <tr>
            <th>Фамилия</th>
            <td>@Model.CurrentContact.Surname</td>
        </tr>
        <tr>
            <th>Отчество</th>
            <td>@Model.CurrentContact.Patronymic</td>
        </tr>
        <tr>
            <th>Номер телефона</th>
            <td>@Model.CurrentContact.PhoneNumber</td>
        </tr>
        <tr>
            <th>Адрес</th>
            <td>@Model.CurrentContact.Address</td>
        </tr>
        <tr>
            <th>Описание</th>
            <td>@Model.CurrentContact.Description</td>
        </tr>
    </table>


</body>
</html>